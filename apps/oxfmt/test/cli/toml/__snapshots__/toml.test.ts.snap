// Vitest Snapshot v1, https://vitest.dev/guide/snapshot.html

exports[`toml > should apply format options 1`] = `
"--- FILE -----------
config.toml
--- BEFORE ---------
[package]
name="test"
version    =    "0.1.0"
description   =  "A test package"

[dependencies]
foo={version="1.0"}
bar   =   {   version   =   "2.0"  ,  features   =   [  "a"  ,  "b"  ]   }

--- AFTER ----------
[package]
name = "test"
version = "0.1.0"
description = "A test package"

[dependencies]
foo = { version = "1.0" }
bar = { version = "2.0", features = [
	"a",
	"b",
] }

--------------------"
`;

