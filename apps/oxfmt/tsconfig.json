{
  "compilerOptions": {
    "module": "Preserve",
    "moduleResolution": "Bundler",
    "noEmit": true,
    "target": "ESNext",
    "strict": true,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "types": ["node"]
  },
  "include": ["src-js", "test/**/*.ts"],
  "exclude": ["test/**/fixtures"]
}
