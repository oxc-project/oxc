{
  "jsPlugins": [
    "./plugin.ts"
  ],
  "categories": {
    "correctness": "off"
  },
  "rules": {
    "globals-plugin/globals": "error"
  },
  "globals": {
    "React": "readonly",
    "process": "writable",
    "console": "readonly",
    "window": "off"
  },
  "overrides": [
    {
      "files": [
        "files/nested/**"
      ],
      "globals": {
        "React": "writable",
        "process": "off",
        "customGlobal": "readonly"
      }
    }
  ]
}
