{
  "categories": {
    "correctness": "off"
  },
  "env": {
    "jquery": true
  },
  "globals": {
    "Foo": "readonly"
  },
  "rules": {
    "no-global-assign": "warn"
  },
  "overrides": [
    {
      "files": [
        "*.ts"
      ],
      "env": {
        "jquery": false
      },
      "globals": {
        "Foo": "writable"
      }
    },
    {
      "files": [
        "src/*"
      ],
      "env": {
        "jquery": false
      },
      "globals": {
        "Foo": "writable"
      }
    }
  ]
}
