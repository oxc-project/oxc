{
  "compilerOptions": {
    "checkJs": true,
    "moduleResolution": "bundler",
    "lib": ["esnext"],
    "target": "esnext",
    "module": "esnext",
    "strict": true,
    "skipLibCheck": true,
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "rewriteRelativeImportExtensions": true
  }
}
